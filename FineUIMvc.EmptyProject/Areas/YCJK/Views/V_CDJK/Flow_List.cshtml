
@{
    ViewBag.Title = "Flow_List";
   var F = @Html.F();
}
@section head {
<meta charset="UTF-8">
<title>流量列表</title>
<link rel="stylesheet" href="~/res/YCJK/css/reset.css" />
<link rel="stylesheet" href="~/res/YCJK/css/jquery-ui.min.css" />
<link rel="stylesheet" href="~/res/CDJK/css/PA_List.css" />
<link rel="stylesheet" href="~/res/YCJK/css/MapRightChart.css" />
<link rel="stylesheet" href="~/res/CDJK/css/Flow_List.css" />
<link href="~/res/css/radialindicator.css" rel="stylesheet" />
<link rel="stylesheet" href="~/res/YCJK/css/jquery.mCustomScrollbar.min.css" />
<link rel="stylesheet" type="text/css" href="~/res/CDJK/css/reportNavListStyle.css" />
<script type="text/javascript" src="~/res/YCJK/js/jquery-2.1.0.js"></script>
<script type="text/javascript" src="~/res/YCJK/js/echarts.min.js"></script>
<script type="text/javascript" src="~/res/YCJK/js/laydate.js"></script>
<script type="text/javascript" src="~/res/YCJK/js/jquery-ui-1.10.4.min.js"></script>
<script type="text/javascript" src="~/res/YCJK/js/jquery.mousewheel.js"></script>
<script src="~/res/layer/layer.js"></script>
}

@section body {
   <div class="main_wrap clearfix">
    <div class="list_wrap">
        <div class="list_header clearfix">
            <div class="search_box">
                <div class="search_wrap">
                    <button class="onlineBtn">在线</button><button class="offlineBtn">离线</button><button class="alarmBtn">报警</button><button class="showAllBtn active">全部</button>
                    <input id="searchText" type="text" placeholder="搜索内容" />
                </div>
                <button class="searchBtn">搜索</button>
            </div>
        </div>
        <div class="list_table">
            <div class="table1_wrap">

                <table class="myTable table1">
                    <thead>
                        <tr>
                            <th></th>
                            <th></th>
                            <th>采集时间</th>
                            <th>测点编号</th>
                            <th>测点名称</th>
                            <th class="thHide">抄表标识</th>
                            <th class="thHide">记录序号</th>
                            <th>正累积流量(m³)</th>
                            <th>负累积流量(m³)</th>
                            <th>瞬时流量(m³/h)</th>
                            <th>水表电池电压(V)</th>
                            <th>进水压力(MPa)</th>
                            <th>出水压力(MPa)</th>
                            <th>压力设定(MPa)</th>
                            <th>阀门开度(%)</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
                @*<div class="setBtn"></div>*@
            </div>
            <div class="table2_wrap">
                <table class="myTable table2">
                    <thead>
                        <tr>
                            <th></th>
                            <th></th>
                            <th>采集时间</th>
                            <th>测点编号</th>
                            <th>测点名称</th>
                            <th class="thHide">抄表标识</th>
                            <th class ="thHide">记录序号</th>
                            <th>正累积流量(m³)</th>
                            <th>负累积流量(m³)</th>
                            <th>瞬时流量(m³/h)</th>
                            <th>水表电池电压(V)</th>
                            <th>进水压力(MPa)</th>
                            <th>出水压力(MPa)</th>
                            <th>压力设定(MPa)</th>
                            <th>阀门开度(%)</th>
                        </tr>
                    </thead>
                    <tbody>
                      
                    </tbody>
                </table>
            </div>
        </div>
        <div class="page_wrap">
            <div class="page_box">
                <a id="firstPage" href="#">&lt;&lt;</a>
                <a id="prevPage" href="#">&lt;</a><a id="currentPage" href="#">1</a><a href="#">/</a><a id="totalPage" href="#">100</a><a id="nextPage" href="#">&gt;</a><a id="lastPage" href="#">&gt;&gt;</a>
                <span>每页</span><a id="pageSize" href="#">10</a><input type="text" id="editPageIndex" /><span>条，共</span><a id="totalNum" href="#">12222</a><span>条</span>
            </div>
        </div>
    </div>
    <div class="chart_box">
        <div id="showHideBtn" class="show"></div>
        <div class="chart_mainBox">          
            <div class="realPress_wrap">
                <p class="modelText">实时压力曲线图</p>
                <div class=" chart_wrap">
                    <div class="echartsBox" id="chart_realPress"></div>
                </div>
            </div>
            <div class="dayHour_wrap">
                <p class="modelText">日每时用水量</p>
                <div class="chart_wrap">
                    <div class="echartsBox" id="chart_dayHour"></div>
                </div>
            </div>
            <div class="monthDay_wrap">
                <p class="modelText">月每日用水量</p>
                <div class="chart_wrap ">
                    <div class="echartsBox" id="chart_monthDay"></div>
                </div>
            </div>
            <div class="yearMonth_wrap">
                <p class="modelText">年每月用水量</p>
                <div class="chart_wrap">
                    <div class="echartsBox" id="chart_yearMonth"></div>
                </div>
            </div>
        </div>
        <div class="report_wrap">
            <div class="showReportBtn">报表导航</div>
            <div class="reportNav_wrap">
                <div class="reportNav_box">
                    <div class="reportNav_head"><p>报表导航</p></div>
                    <div class="report_listWrap">
                        <ul class="report_list">
                            @*<li data-key="flow_runLog"><i class="lineBar_ico"></i><a>运行日志</a></li>
                            <li data-key="flow_compreReport"><i class="lineBar_ico"></i><a>综合报表</a></li>
                            <li><i class="lineBar_ico"></i><a>实时压力曲线图</a></li>
                            <li><i class="lineBar_ico"></i><a>日时段压力曲线图</a></li>
                            <li><i class="area_ico"></i><a>日时段电量统计图</a></li>
                            <li><i class="area_ico"></i><a>日时段流量统计图</a></li>
                            <li><i class="lineBar_ico"></i><a>月每日压力曲线图</a></li>
                            <li><i class="lineBar_ico"></i><a>月每日电量统计图</a></li>
                            <li><i class="lineBar_ico"></i><a>月每日流量统计图</a></li>
                            <li><i class="lineBar_ico"></i><a>年每月压力曲线图</a></li>
                            <li><i class="lineBar_ico"></i><a>年每月电量统计图</a></li>
                            <li><i class="lineBar_ico"></i><a>年每月流量统计图</a></li>
                            <li><i class="lineBar_ico"></i><a>水泵小保提示报表</a></li>
                            <li><i class="lineBar_ico"></i><a>水泵大保提示报表</a></li>
                            <li><i class="lineBar_ico"></i><a>吨水能耗</a></li>*@
                        </ul>
                    </div>
                </div>
                <div class="hideReportBtn">向上收缩隐藏</div>
            </div>
        </div>
    </div>
</div>
}
@section script {
  <script type="text/javascript" src="~/res/configJson/reportList.js"></script>
<script type="text/javascript" src="~/res/CDJK/js/Flow_List.js"></script>
<script type="text/javascript" src="~/res/CDJK/js/reportNavList.js"></script>
<script type="text/javascript" src="~/res/YCJK/js/jquery.mCustomScrollbar.js"></script>
<script src="~/res/js/radialIndicator.js"></script>
}
