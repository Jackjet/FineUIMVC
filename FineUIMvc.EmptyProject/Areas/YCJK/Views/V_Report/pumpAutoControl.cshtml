
@{
    ViewBag.Title = "pumpAutoControl";
    var F = @Html.F();
}
@section head {
    <title>泵房启停控制</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" type="text/css" href="~/res/YCJK/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="~/res/YCJK/css/jquery.mCustomScrollbar.min.css" />
<link rel="stylesheet" type="text/css" href="~/res/YCJK/css/pumpAutoControl.css" />
    <script src="~/res/YCJK/js/jquery-2.1.0.js"></script>
    <script src="~/res/js/echarts.min.js"></script>
    <script type="text/javascript" src="~/res/YCJK/js/laydate.js"></script>
    <script type="text/javascript" src="~/res/YCJK/js/jquery-ui-1.10.4.min.js"></script>
    <script type="text/javascript" src="~/res/YCJK/js/jquery.mousewheel.js"></script>
    <script src="~/res/layer/layer.js"></script>
}
@section body {
    <div class="header_wrap">
        <div class="autoControl">自动控制</div>
        <div class="menufControl">手动控制</div>
        <div class="remoteAuto">远程自动控制</div>
    </div>
    <div class="main_wrap">
        <div class="waterIn_wrap">
        <label>时段1压力上限：</label><input type="text" class="upLimit" /> <label>下限：</label><input type="text" class="downLimit" /> <button class="accBtn">确定</button>
        </div>
        <div class="waterOut_wrap">
            <label>时段2压力上限：</label><input type="text" class="upLimit" /> <label>下限：</label><input type="text" class="downLimit" /> <button class="accBtn">确定</button>
        </div>
    </div>
}
@section script {
    <script type="text/javascript" src="~/res/YCJK/js/pumpAutoControl.js"></script>
    <script type="text/javascript" src="~/res/YCJK/js/jquery.mCustomScrollbar.js"></script>
}
