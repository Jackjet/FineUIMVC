
@{
    ViewBag.Title = "HS_S1";
}

@section head {

    <title>泵房设备</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" type="text/css" href="~/res/YCJK/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="~/res/YCJK/css/jquery.mCustomScrollbar.min.css" />
    <script src="~/res/YCJK/js/jquery-2.1.0.js"></script>
    <script src="~/res/YCJK/js/lowCan.js"></script>
    <link rel="stylesheet" href="~/res/hs_shuichang/s1/css/S1_MainStyle.css" />
    <link rel="stylesheet" href="~/res/YCJK/css/MidChart.css" />
    <link href="~/res/hs_shuichang/s1/css/pageRightDataBox.css" rel="stylesheet" />
    <link href="~/res/YCJK/css/pageRightSetBox.css" rel="stylesheet" />
    <link href="~/res/css/radialindicator.css" rel="stylesheet" />
    <script src="http://api.map.baidu.com/api?v=2.0&ak=lSs5x0CtdFvp81zUi3sxZo7Zcxm2BC8X"></script>
    <script src="~/res/layer/layer.js"></script>
    <script src="~/res/js/echarts.min.js"></script>

}

@section body {
    <div class="pageLeft" data-sid="@ViewBag.ShuiChangID">

    <div class="pageLeftMid">
        @*<iframe class="shebeiFrame" src="" frameborder="0"></iframe>*@
        <div class="highWrap active">


            <div class="sCBox clearfix">
                <div class="sCState "></div>
                <div class="sCName"></div>
            </div> 
            <div class="threeSection">
                <div class="threePicBox">
                    <div class="layoutwarp" id="layoutwarp">

                        <div class="statePicBox">@*<div class="pic1"></div>*@</div>


                        @* 左上的 进水压力框*@
                        <div class="inPressBoxleft">
                            <div class="inPressCurrent clearfix"><div class="inCurrentName">进水瞬时:</div><div class="inCurrentNum shuiChuagNum">123.7</div></div>
                            <div class="inPressTotal clearfix"><div class="inTotalName">进水累计:</div><div class="inTotalNum shuiChuagNum">123.7</div></div>
                        </div>

                        @*左上水位*@

                        <div class="leftShuiWei clearfix"><div class="leftShuiWeiName">水池水位:</div><div class="leftShuiWeiNum shuiChuagNum">123.7</div></div>
                        @*左上氯气*@

                        <div class="leftLv clearfix"><div class="leftDanName">氯气重量:</div><div class="leftDanNum shuiChuagNum">123.7</div></div>


                        @*右侧压力框1*@

                        <div class="outPressRight outPressRight1">
                            <div class="outPressCurrent outPressCurrent1 clearfix"><div class="outCurrentName">出水瞬时1:</div><div class="outCurrentNum shuiChuagNum">123.7</div></div>
                            <div class="outPressTotal outPressTotal1 clearfix"><div class="outTotalName">出水累计1:</div><div class="outTotalNum shuiChuagNum">123.7</div></div>
                        </div>

                        @*右侧压力框2*@

                        <div class="outPressRight outPressRight2">
                            <div class="outPressCurrent outPressCurrent2 clearfix"><div class="outCurrentName">出水瞬时2:</div><div class="outCurrentNum shuiChuagNum">123.7</div></div>
                            <div class="outPressTotal outPressTotal2 clearfix"><div class="outTotalName">出水累计2:</div><div class="outTotalNum shuiChuagNum">123.7</div></div>
                        </div>


                        @*出水压力*@
                        <div class="outRight clearfix"><div class="outRightName">出水压力:</div><div class="outRightNum shuiChuagNum">123.7</div></div>

                        @*泵的  功率  和  电量*@
                        <div class="gongLvBox gongLvBox1">160kw</div>
                        <div class="gongLvBox gongLvBox2">75kw</div>
                        <div class="gongLvBox gongLvBox3">160kw</div>
                        <div class="gongLvBox gongLvBox4">200kw</div>
                        <div class="gongLvBox gongLvBox5">160kw</div>
                        <div class="gongLvBox gongLvBox6">75kw</div>

                        <div class="dianLiangBox dianLiangBox1">160A</div>
                        <div class="dianLiangBox dianLiangBox2">75A</div>
                        <div class="dianLiangBox dianLiangBox3">160A</div>
                        <div class="dianLiangBox dianLiangBox4">200A</div>
                        <div class="dianLiangBox dianLiangBox5">160A</div>
                        <div class="dianLiangBox dianLiangBox6">75A</div>




                        @*每个泵的  叶轮*@
                        <div class="yelunItem yeLun1 run"></div>
                        <div class="yelunItem yeLun2"></div>
                        <div class="yelunItem yeLun3"></div>
                        <div class="yelunItem yeLun4 error"></div>
                        <div class="yelunItem yeLun5"></div>
                        <div class="yelunItem yeLun6"></div>
                    </div>
                </div>
            </div>

        </div>

    </div>
    <div class="pageLeftBot hide">
        <div class="botArrow"></div>
        <ul class="leftBotNav clearfix">
            <li class="clearfix">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">水厂信息</div>
            </li>
            <li class="clearfix">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">运行日志</div>
            </li>
            <li class="clearfix ">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">维护巡检</div>
            </li>
            <li class="clearfix">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">数据报表</div>
            </li>

            <li class="clearfix">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">预警报警</div>
            </li>
            <li class="clearfix">
                <div class="leftBotNavPic"></div>
                <div class="leftBotNavTxt">相关附件</div>
            </li>

        </ul>
        <div class="leftBotBox">

            <iframe class="leftBotFrame" src="" frameborder="0"></iframe>
        </div>
    </div>
</div>
    <div class="pageMid">
        <div class="showHideBtn showHideBtn1"></div>
        <div class="pageMidBox">
           
        </div>
    </div>
    <div class="pageRight">
        <div class="showHideBtn showHideBtn2"></div>
        <div class="pageRightBox">
            <div class="pageRightTop">
                <div class="caiJi"></div>
               @* <div class="xianShi"></div>*@
                <ul class="rightTopTab clearfix">
                    <li class="rightTopTabItem clearfix active">
                        <div class="rightTopTabItemPic"></div>
                        <div class="rightTopTabItemTxt">数据</div>
                        <!--</li>
                        <li class="rightTopTabItem clearfix">
                            <div class="rightTopTabItemPic"></div>
                            <div class="rightTopTabItemTxt">控制</div>
                        </li>-->
                    <li class="rightTopTabItem clearfix">
                        <div class="rightTopTabItemPic"></div>
                        <div class="rightTopTabItemTxt">设置</div>
                    </li>
                </ul>
                <div class="rightTopMainBox">
                    <div class="dataBox active">
                        <!--1-->
                        <div class="getDataTime">
                            <div class="getDataPic"></div>
                            <div class="getDataTxt"> 采集时间： <span>2017-05-02 17:20:01</span></div>
                        </div>

                        

                        <div class="inFoBox">
                            <ul class="infoList">
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水压力:</div>
                                    <div class="infoItemTxt outRight"></div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">PH值:</div>
                                    <div class="infoItemTxt PHZhi">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水浊度:</div>
                                    <div class="infoItemTxt outZhuo">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水余氯:</div>
                                    <div class="infoItemTxt outLv">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水瞬时流量1:</div>
                                    <div class="infoItemTxt outPressCurrent1">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水累计流量1:</div>
                                    <div class="infoItemTxt outPressTotal1">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水瞬时流量2:</div>
                                    <div class="infoItemTxt outPressCurrent2">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">出水累计流量2:</div>
                                    <div class="infoItemTxt outPressTotal2">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">清水池水位:</div>
                                    <div class="infoItemTxt leftShuiWei">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">进水瞬时流量:</div>
                                    <div class="infoItemTxt inPressCurrent">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">进水累计流量:</div>
                                    <div class="infoItemTxt inPressTotal">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">氯气重量:</div>
                                    <div class="infoItemTxt leftLv">312</div>
                                </li>
                                <li class="infoItem">
                                    <div class="infoItemPic"></div>
                                    <div class="infoItemName">频率:</div>
                                    <div class="infoItemTxt pinLv">312</div>
                                </li>
                                
                            </ul>
                        </div>
                     
                 
                    </div>
                    <div class="setBox  ">

                        

                    </div>
                </div>
            </div>
            <div class="pageRightBot">
                <div class="mapBoxShowHide show"></div>
                <ul class="rightBotTab clearfix">
                    <li class="rightBotTabItem clearfix active">
                        <div class="rightBotTabItemPic"></div>
                        <div class="rightBotTabItemTxt">地图</div>
                    </li>
                    <li class="rightBotTabItem clearfix">
                        <div class="rightBotTabItemPic"></div>
                        <div class="rightBotTabItemTxt">视频</div>
                    </li>
                </ul>
                <div class="rightBotMainBox">
                    <div id="mapBox" class="active"></div>
                    <div class="videoBox"></div>
                </div>
            </div>
        </div>
    </div>
    <ul class="rightShowBtn">
        <li class="midShow"></li>
        <li class="rightShow"></li>
    </ul>
}
@section script {
    @*<script src="~/res/YCJK/js/flow.js"></script>*@
    <script src="~/res/hs_shuichang/s1/js/flowS1.js"></script>
    <script src="~/res/YCJK/js/MidChart.js"></script>
   <script src="~/res/hs_shuichang/s1/js/Hs_s1Main.js"></script>
    <script src="~/res/YCJK/js/jquery.mCustomScrollbar.js"></script>
    <script src="~/res/YCJK/js/jquery.mousewheel.js"></script>
    <script src="~/res/js/radialIndicator.js"></script>

}

